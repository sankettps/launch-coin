
<div class="fix-view-banner">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="login-view">
          <% if resource.errors.present? %>
            <div class="alert alert-danger alert-dismissible">
              <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
              <%= resource.errors.full_messages.map { |msg| content_tag(:p, msg) }.join.html_safe %>
            </div>
          <% end %>
          <div class="title">
            <h3>Edit Profile</h3>
          </div>
          <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { method: :put }) do |f| %>
            <div class="input">
                <%#= f.label "#{resource.email}", autocomplete: "email",class: 'input-field'%>
                <label class="input-field"><%= resource.email %></label>
            </div>
            <div class="input">
              <%= f.text_field :first_name, class: 'input-field' %>
              <label class="input-label input-label-juro">First Name</label>
            </div>
            <div class="input">
              <%= f.text_field :last_name, class: 'input-field' %>
              <label class="input-label input-label-juro">Last Name</label>
            </div>
            <div class="input">
              <%= f.text_field :mobile_no, class: 'input-field' %>
              <label class="input-label input-label-juro">Mobine No</label>
            </div>
            <div class="input">
                <%= f.password_field :password, autocomplete: "off",class: 'input-field' %>
                <label class="input-label input-label-juro">Password</label>
            </div>
            <div class="input">
              <%= f.password_field :password_confirmation,class: 'input-field' %>
                <label class="input-label input-label-juro">Confirm Password</label>
            </div>
            <div class="input">
              <%= f.password_field :current_password,class: 'input-field' %>
                <label class="input-label input-label-juro">Current Password</label>
            </div>
            <div class="submit-btn">
              <button type="submit" class="submit-btn">Update Profile</button>
              <%#= f.submit "SIGN IN",class: "submit-btn" %>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>





