<div class="coin-list">
  <div class="fix-view">
    <div class="owl-carousel owl-theme" id="owl">
     <div class="item">
          <img src="/images/banner-3.png" class="fix-img" />
      </div>
    </div>
     <div id="particles-js"></div>
    <div class="banner flex-box" >
      <h1>Start Your Dream </h1>
      <p>Cryptotoken sale  with Icototes.</p>
      <a href="#">Upload Ico</a>
    </div>
  </div>

  <div class="coin-list pad-block">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <div class="title">
            <h3>Admin Coins</h3>
          </div>
        </div>
        <div class="col-xs-12">
          <div class="admin-coin">
            <ul class="nav nav-tabs">
              <li class="active"><a data-toggle="tab" href="#pending">Pending</a></li>
              <li><a data-toggle="tab" href="#approved">Approved</a></li>
              <li><a data-toggle="tab" href="#declined">Declined</a></li>
            </ul>

            <div class="tab-content">
              <div id="pending" class="tab-pane fade in active">
                <h3>Pending Icos</h3>
                <%= render partial: 'ico_list' %>
              </div>
              <div id="approved" class="tab-pane fade">
                <h3>Approved Icos</h3>
                <%= render partial: 'approved_icos' %>
              </div>
              <div id="declined" class="tab-pane fade">
                <h3>Declined Icos</h3>
                <%= render partial: 'declined_icos' %>
              </div>
              <div id="menu3" class="tab-pane fade">
                <h3>Menu 3</h3>
                <p>Eaque ipsa quae ab illo inventore veritatis et quasi architecto beatae vitae dicta sunt explicabo.</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function() {
    $('.list-table').DataTable();
  });
</script>