  });
  $('#enddatepicker1').datetimepicker({
    format: 'DD/MM/YYYY HH:mm',
    useCurrent: false
  });
  $('#startdatepicker1').on('dp.change', function(e) {
    $('#enddatepicker1').data('DateTimePicker').minDate(e.date);
