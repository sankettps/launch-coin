<%= form_with(model: coin_ico, local: true) do |form| %>
  <% if coin_ico.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(coin_ico.errors.count, "error") %> prohibited this coin_ico from being saved:</h2>

      <ul>
      <% coin_ico.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <!-- <div class="frm-wrap"> -->
          <!-- progressbar -->
  <div class="field">
    <%= form.hidden_field :launcher_id, id: :coin_ico_launcher_id,value: Launcher.first.try(:id) %>
  </div>

  <div class="field">
    <%= form.label :coin_name %>
    <%= form.text_field :coin_name, id: :coin_ico_coin_name %>
  </div>

  <div class="field">
    <%= form.label :rate %>
    <%= form.number_field :rate, id: :coin_ico_rate %>
  </div>

  <div class="field">
    <%= form.label :total_supply %>
    <%= form.number_field :total_supply, id: :coin_ico_total_supply %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description, id: :coin_ico_description %>
  </div>

  <div class="field">
    <%= form.label :website %>
    <%= form.text_field :website, id: :coin_ico_website %>
  </div>


  <div class="field">
    <%= form.label :presentation_link %>
    <%= form.text_field :presentation_link, id: :coin_ico_presentation_link %>
  </div>

  <div class="field">
    <%= form.label :facebook_link %>
    <%= form.text_field :facebook_link, id: :coin_ico_facebook_link %>
  </div>

  <div class="field">
    <%= form.label :twitter_link %>
    <%= form.text_field :twitter_link, id: :coin_ico_twitter_link %>
  </div>

  <div class="field">
    <%= form.label :telegram_link %>
    <%= form.text_field :telegram_link, id: :coin_ico_telegram_link %>
  </div>

  <div class="field">
