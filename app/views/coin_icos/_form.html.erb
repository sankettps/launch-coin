  </div>

  <div class="field">
    <%= form.label :accepts %>
    <%= form.text_field :accepts, id: :coin_ico_accepts %>
  </div>

  <div class="field">
    <%= form.label :ico_end_date %>
    <%= form.datetime_select :ico_end_date, id: :coin_ico_ico_end_date %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
  </div>
<% end %>
